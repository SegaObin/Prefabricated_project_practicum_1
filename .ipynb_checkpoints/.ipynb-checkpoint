    "genre_share_df = (data\n",
    "                  .loc[data['year_of_release'] >= 2014]                    \n",
    "                  .groupby('genre')[['na_sales', 'eu_sales', 'jp_sales']] \n",
    "                  .agg('sum')                                               \n",
    "                  .reset_index()\n",
    "                 )                                                           \n",
    "genre_share_df"
   ]
