   "source": [
    "top_platforms = (data\n",
    "                 .pivot_table(\n",
    "                     index='platform',\n",
    "                     values='total_sales',\n",
    "                     aggfunc='sum'\n",
    "                 )\n",
    "                 .sort_values(\n",
    "                     by='total_sales',\n",
    "                     ascending=False\n",
    "                 )\n",
    "                 .head(20)\n",
    ")\n",
    "plt.figure(figsize=(15, 8))\n",
    "sns.barplot(data=top_platforms, x=top_platforms.index, y=top_platforms['total_sales'])\n",
